<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/HomeFragment">

    <fragment
        android:id="@+id/HomeFragment"
        android:name="com.side.runwithme.view.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_recommendFragment"
            app:destination="@id/recommendFragment" />
        <action
            android:id="@+id/action_homeFragment_to_challengeListFragment"
            app:destination="@id/challengeListFragment" />
        <action
            android:id="@+id/action_homeFragment_to_MyPageFragment"
            app:destination="@id/MyPageFragment" />

    </fragment>
    <fragment
        android:id="@+id/challengeListFragment"
        android:name="com.side.runwithme.view.challenge.ChallengeListFragment"
        android:label="ChallengeListFragment">
        <action
            android:id="@+id/action_challengeListFragment_to_challengeSearchFragment"
            app:destination="@id/challengeSearchFragment" />
        <action
            android:id="@+id/action_challengeListFragment_to_challengeCreateStep1Fragment"
            app:destination="@id/challengeCreateStep1Fragment" />
    </fragment>
    <fragment
        android:id="@+id/challengeSearchFragment"
        android:name="com.side.runwithme.view.challenge.ChallengeSearchFragment"
        android:label="ChallengeSearchFragment" />
    <fragment
        android:id="@+id/recommendFragment"
        android:name="com.side.runwithme.view.recomend.RecommendFragment"
        android:label="RecommendFragment">
        <action
            android:id="@+id/action_recommendFragment_to_recommendDetailFragment"
            app:destination="@id/recommendDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/recommendDetailFragment"
        android:name="com.side.runwithme.view.recommend_detail.RecommendDetailFragment"
        android:label="RecommendDetailFragment">
        <action
            android:id="@+id/action_recommendDetailFragment_to_routeDetailFragment"
            app:destination="@id/routeDetailFragment" />
        <action
            android:id="@+id/action_recommendDetailFragment_to_favoriteDialog"
            app:destination="@id/favoriteDialog" />
    </fragment>
    <fragment
        android:id="@+id/routeDetailFragment"
        android:name="com.side.runwithme.view.route_detail.RouteDetailFragment"
        android:label="RouteDetailFragment" />
    <dialog
        android:id="@+id/favoriteDialog"
        android:name="com.side.runwithme.view.recommend_detail.favorite.FavoriteDialog"
        android:label="FavoriteDialog" />
    <fragment
        android:id="@+id/MyPageFragment"
        android:name="com.side.runwithme.view.my_page.MyPageFragment"
        android:label="MyPageFragment" />
    <fragment
        android:id="@+id/challengeCreateStep1Fragment"
        android:name="com.side.runwithme.view.challenge.create.ChallengeCreateStep1Fragment"
        android:label="ChallengeCreateStep1Fragment" >
        <action
            android:id="@+id/action_challengeCreateStep1Fragment_to_challengeCreateStep2Fragment"
            app:destination="@id/challengeCreateStep2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/challengeCreateStep2Fragment"
        android:name="com.side.runwithme.view.challenge.create.ChallengeCreateStep2Fragment"
        android:label="ChallengeCreateStep2Fragment" >
        <action
            android:id="@+id/action_challengeCreateStep2Fragment_to_challengeCreateStep3Fragment"
            app:destination="@id/challengeCreateStep3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/challengeCreateStep3Fragment"
        android:name="com.side.runwithme.view.challenge.create.ChallengeCreateStep3Fragment"
        android:label="ChallengeCreateStep3Fragment" >
        <action
            android:id="@+id/action_challengeCreateStep3Fragment_to_challengeCreateStep4Fragment"
            app:destination="@id/challengeCreateStep4Fragment" />
    </fragment>
    <fragment
        android:id="@+id/challengeCreateStep4Fragment"
        android:name="com.side.runwithme.view.challenge.create.ChallengeCreateStep4Fragment"
        android:label="ChallengeCreateStep4Fragment" >
        <action
            android:id="@+id/action_challengeCreateStep4Fragment_to_challengeCreateStep5Fragment"
            app:destination="@id/challengeCreateStep5Fragment" />
    </fragment>
    <fragment
        android:id="@+id/challengeCreateStep5Fragment"
        android:name="com.side.runwithme.view.challenge.create.ChallengeCreateStep5Fragment"
        android:label="ChallengeCreateStep5Fragment" >
        <action
            android:id="@+id/action_challengeCreateStep5Fragment_to_challengeListFragment"
            app:destination="@id/challengeListFragment"
            app:popUpTo="@layout/fragment_challenge_list"
            app:popUpToInclusive="true"/>
    </fragment>
</navigation>